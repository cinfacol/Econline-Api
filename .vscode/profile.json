{
    "name": "jorge-vscode",
    "settings": {
        "workbench.sideBar.location": "right",
        "window.newWindowProfile": "jorge-vscode",
        "security.workspace.trust.untrustedFiles": "open",
        "editor.fontFamily": "JetBrains Mono, 'Courier New', monospace",
        "editor.fontSize": 14,
        "editor.lineHeight": 1.5,
        "editor.cursorBlinking": "smooth",
        "editor.cursorSmoothCaretAnimation": "on",
        "editor.smoothScrolling": true,
        "editor.minimap.enabled": true,
        "editor.minimap.renderCharacters": false,
        "editor.minimap.maxColumn": 80,
        "editor.bracketPairColorization.enabled": true,
        "editor.guides.bracketPairs": true,
        "editor.wordWrap": "on",
        "editor.renderWhitespace": "boundary",
        "editor.renderControlCharacters": true,
        "editor.mouseWheelZoom": true,
        "editor.multiCursorModifier": "alt",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.formatOnType": true,
        "editor.suggestSelection": "first",
        "editor.acceptSuggestionOnEnter": "on",
        "editor.snippetSuggestions": "top",
        "editor.tabCompletion": "on",
        "editor.wordBasedSuggestions": "on",
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        },
        "workbench.editor.enablePreview": true,
        "workbench.editor.enablePreviewFromQuickOpen": true,
        "workbench.editor.highlightModifiedTabs": true,
        "workbench.editor.showTabs": "multiple",
        "workbench.editor.tabSizing": "shrink",
        "workbench.editor.wrapTabs": false,
        "workbench.editor.decorations.badges": true,
        "workbench.editor.decorations.colors": true,
        "workbench.iconTheme": "material-icon-theme",
        "workbench.colorTheme": "Material Theme",
        "workbench.startupEditor": "none",
        "workbench.tree.indent": 20,
        "workbench.tree.renderIndentGuides": "always",
        "workbench.tree.expandMode": "doubleClick",
        "workbench.tree.enableStickyScroll": true,
        "workbench.tree.collapseOnFocusOut": false,
        "workbench.tree.enableFilterOnType": true,
        "workbench.tree.autoReveal": true,
        "workbench.tree.autoRevealOnFocus": true,
        "workbench.tree.autoRevealOnFocusOut": false,
        "workbench.tree.autoRevealOnFocusIn": true,
        "workbench.tree.autoRevealOnFocusOutDelay": 500,
        "workbench.tree.autoRevealOnFocusInDelay": 500,
        "workbench.tree.autoRevealOnFocusOutDuration": 300,
        "workbench.tree.autoRevealOnFocusInDuration": 300,
        "workbench.tree.autoRevealOnFocusOutEasing": "easeOutCubic",
        "workbench.tree.autoRevealOnFocusInEasing": "easeOutCubic",
        "workbench.tree.autoRevealOnFocusOutMaxDuration": 300,
        "workbench.tree.autoRevealOnFocusInMaxDuration": 300,
        "workbench.tree.autoRevealOnFocusOutMinDuration": 100,
        "workbench.tree.autoRevealOnFocusInMinDuration": 100,
        "workbench.tree.autoRevealOnFocusOutMaxDelay": 500,
        "workbench.tree.autoRevealOnFocusInMaxDelay": 500,
        "workbench.tree.autoRevealOnFocusOutMinDelay": 0,
        "workbench.tree.autoRevealOnFocusInMinDelay": 0
    }
}
